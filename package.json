{
  "name": "re-toaster",
  "version": "1.0.6",
  "description": "React Redux Toaster Component",
  "main": "lib/index.js",
  "scripts": {
    "try-code-in-demo": "rm -rf demo/src/lib && cp -r src demo/src/lib",
    "compile": "babel --presets es2015,stage-0 -d lib/ src/",
    "prepublish": "npm run compile",
    "prepublish-and-move-to-demo": "npm run compile && cp -r lib demo/src/lib",
    "watch": "gulp watch",
    "test": "ava"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.{js,es}",
      "!test/__lib__/**/*"
    ],
    "source": [
      "**/*.{js,jsx,es}",
      "!build/**/*"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "extension": [
      ".jsx",
      ".es"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sunny-fr/re-toaster.git"
  },
  "author": "Alaa El Helw",
  "license": "ISC",
  "keywords": [
    "react",
    "redux"
  ],
  "bugs": {
    "url": "https://github.com/Sunny-fr/re-toaster/issues"
  },
  "homepage": "https://github.com/Sunny-fr/re-toaster#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "ava": "^0.16.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-watch": "^4.3.9",
    "nyc": "^8.1.0"
  },
  "dependencies": {
    "deepmerge": "^1.5.0",
    "prop-types": "^15.5.10",
    "react": "^16.12.0",
    "react-redux": "^5.0.2",
    "redux": "^4.0.5"
  }
}
